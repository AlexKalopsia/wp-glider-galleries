{
  "apiVersion": 2,
  "name": "glider/gallery",
  "title": "Glider Gallery",
  "description": "Simple media gallery overriding pre-existing Jetpack slideshows.",
  "category": "media",
  "icon": "images-alt2",
  "version": "0.1",
  "textdomain": "wp-glider-galleries",
  "editorScript": "glider-gallery-block",
  "editorStyle": "glider-block-editor-styles",
  "attributes": {
    "ids": {
      "type": "array",
      "default": []
    }
  },
  "supports": {
    "align": ["wide", "full", "center"],
    "html": false,
    "preview": true
  },
  "example": {
    "attributes": {
      "ids": [1, 2, 3]
    }
  }
}
